{% extends "CvticTddDemoBundle::layout.html.twig" %}
{% block title "Formulaire de contact" %}
{% block content %}
    <h2>Ajouter un contact</h2>
    <form action="{{ path('_demo_contact') }}" method="POST" id="contact_form">
        {{ form_errors(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.email) }}
        {{ form_rest(form) }}
    <input type="submit" value="Ajouter" class="symfony-button-grey" />
</form>
{% endblock %}